{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "",
      "kebab-case"
    ],
    "variable-name": false,
    "member-ordering": false,
    "no-output-on-prefix": false,
    "quotemark": false
  }
}